package com.binary.tables;

import java.util.ArrayList;
import java.util.Scanner;

public class Tables {
	char[] input;
	Scanner keyboard;
	ArrayList<Character> values = new ArrayList<>();
	public static void main(String[] args) {
		new Tables();
	}
	
	public Tables(){
		keyboard = new Scanner(System.in);
		input = keyboard.nextLine().toCharArray();
		for(char c:input){
			boolean alreadyAdded = false;
			if(c!='+' && c!='(' && c!=')' && c!='¬'){
				for(char ceval:values){
					if(c == ceval){
						alreadyAdded = true;
					}
				}
				if(!alreadyAdded){
					values.add(c);
				}
			}
		}
		System.out.println(values.toString());
		for(int i = 0; i<Math.pow(2, values.size());i++){
			ArrayList<Integer> subValues = new ArrayList<>();
			String bin = Integer.toBinaryString(i);
			while(bin.length()<values.size()){
				bin = "0"+bin;
			}
			for(int u = 0; u<values.size(); u++){
				subValues.add((int) bin.charAt(u));
			}
			System.out.println(sub(subValues));
		}
	}
	
	public String sub(ArrayList<Integer> subValues){
		for(int i = 0;i<input.length;i++){
			for(int u = 0;u<values.size();u++){
				if(input[i] == values.get(u)){
					if(i>0){
						if(input[i-1] == '¬'){
							input[i] = (char) (subValues.get(u)==1 ? 0:1);
						}
					}
					input[i] = (char)(int)subValues.get(u);
				}
			}
		}
		return null;
	}
	
	public void pass(){
		
	}
}
